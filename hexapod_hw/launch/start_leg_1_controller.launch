<launch>
    <!-- Start tilt joint controller -->
    <rosparam file="$(find hexapod_hw)/config/controllers.yaml" command="load"/>
    <node name="leg_1_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port hexapod_port
                joint_3_controller
                joint_4_controller
                joint_5_controller"
          output="screen"/>
</launch>
